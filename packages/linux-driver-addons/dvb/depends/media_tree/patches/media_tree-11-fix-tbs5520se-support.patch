+				.count = 8,
+				.endpoint = 0x82,
+				.u = {
+					.bulk = {
+						.buffersize = 4096,
+					}
+				}
+			},
+		},
+/*			{
+				.frontend_attach = tbs5520se_satfrontend_attach,
+				.streaming_ctrl = NULL,
+				.tuner_attach = NULL,
+				.stream = {
+					.type = USB_BULK,
+					.count = 8,
+					.endpoint = 0x82,
+					.u = {
+						.bulk = {
+							.buffersize = 4096,
+						}
+					}
+				},
+			},*/
+
+		},
+		
+	}},
+
+	.num_device_descs = 1,
+	.devices = {
+		{"TurboSight TBS 5520SE",
+			{&tbs5520se_table[0], NULL},
+			{NULL},
+		}
+	}
+};
